<% include header.ejs %>

<script>
  $(function(){
    
    function getHashParams() {
      var hashParams = {};
      var e, r = /([^&;=]+)=?([^&;]*)/g,
          q = window.location.hash.substring(1);
      while ( e = r.exec(q)) {
         hashParams[e[1]] = decodeURIComponent(e[2]);
      }
      return hashParams;
    }
    
    try {
      window.opener.codeReceiver(getHashParams());
    }
    catch (err) {}
    window.close();
    return false;
  });
</script>

